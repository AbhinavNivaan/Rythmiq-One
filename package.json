{
  "name": "rythmiq-one-api-gateway",
  "version": "1.0.0",
  "description": "Rythmiq One API Gateway - Phase 1.5 Track C",
  "main": "dist/api-gateway/server.js",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "npm run build && echo '--- DIST CONTENTS ---' && ls -R dist",
    "start": "node dist/api-gateway/server.js",
    "dev": "ts-node api-gateway/server.ts",
    "worker": "node dist/worker/entrypoint.js",
    "worker:dev": "ts-node worker/entrypoint.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.972.0",
    "@aws-sdk/lib-storage": "^3.972.0",
    "@supabase/supabase-js": "^2.91.0",
    "dotenv": "^17.2.3",
    "expo": "^54.0.31",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "tesseract.js": "^7.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.19.27",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}