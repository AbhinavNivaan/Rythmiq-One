{
    "name": "rythmiq-one-worker",
    "type": "service",
    "engineType": "base",
    "runtime": "nodejs18",
    "command": "node dist/worker/entrypoint.js",
    "resources": {
        "cpu": 1,
        "memory": "2Gi"
    },
    "env": {
        "SERVICE_ENV": "prod",
        "EXECUTION_BACKEND": "camber",
        "JWT_PUBLIC_KEY": "${JWT_PUBLIC_KEY}",
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}",
        "ARTIFACT_STORE_TYPE": "spaces",
        "DO_SPACES_KEY": "${DO_SPACES_KEY}",
        "DO_SPACES_SECRET": "${DO_SPACES_SECRET}",
        "DO_SPACES_BUCKET": "rythmiq-one-artifacts",
        "DO_SPACES_REGION": "sgp1"
    }
}