# Rythmiq One FastAPI Gateway - Environment Template

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# DigitalOcean Spaces
DO_SPACES_ENDPOINT=https://nyc3.digitaloceanspaces.com
DO_SPACES_REGION=nyc3
DO_SPACES_BUCKET=rythmiq-uploads
DO_SPACES_ACCESS_KEY=your-access-key
DO_SPACES_SECRET_KEY=your-secret-key

# Camber
CAMBER_API_URL=https://api.camber.dev
CAMBER_API_KEY=your-camber-api-key
CAMBER_APP_NAME=rythmiq-worker-python-v2

# Service
SERVICE_ENV=dev
WEBHOOK_SECRET=your-webhook-secret

# URL Expiry (seconds)
UPLOAD_URL_EXPIRY_SECONDS=3600
DOWNLOAD_URL_EXPIRY_SECONDS=3600
